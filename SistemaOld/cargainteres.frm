Private Sub Proceso_Click()

    On Error GoTo WError

    Call Limpia_Vector

    Renglon = 0
    Erase Auxiliar
    
    ZSql = ""
    ZSql = ZSql + "Select *"
    ZSql = ZSql + " FROM CtaCtePrv"
    ZSql = ZSql + " Where CtaCtePrv.Proveedor = " + "'" + "10077777777" + "'"
    ZSql = ZSql + " Order by OrdFechaOriginal"
    spCtaCtePrv = ZSql
    Set rstCtaCtePrv = db.OpenRecordset(spCtaCtePrv, dbOpenSnapshot, dbSQLPassThrough)
    If rstCtaCtePrv.RecordCount > 0 Then
        With rstCtaCtePrv
            .MoveFirst
            If .NoMatch = False Then
                Do
                
                    ZZSaldo = IIf(IsNull(rstCtaCtePrv!Saldo), "0", rstCtaCtePrv!Saldo)
                    ZZInteres = IIf(IsNull(rstCtaCtePrv!Interes), "0", rstCtaCtePrv!Interes)
                    
                    If ZZSaldo <> 0 And ZZInteres = 0 Then
                    
                        Renglon = Renglon + 1
                
                        WVector1.TextMatrix(Renglon, 1) = rstCtaCtePrv!fechaoriginal
                        WVector1.TextMatrix(Renglon, 2) = rstCtaCtePrv!desproveoriginal
                        WVector1.TextMatrix(Renglon, 3) = rstCtaCtePrv!facturaoriginal
                        WVector1.TextMatrix(Renglon, 4) = rstCtaCtePrv!cuota
                        WVector1.TextMatrix(Renglon, 5) = rstCtaCtePrv!Fecha
                        WVector1.TextMatrix(Renglon, 6) = Str$(rstCtaCtePrv!Saldo)
                        WVector1.TextMatrix(Renglon, 6) = Pusing("###,###.##", WVector1.TextMatrix(Renglon, 6))
                        WVector1.TextMatrix(Renglon, 7) = Str$(rstCtaCtePrv!Interes)
                        WVector1.TextMatrix(Renglon, 7) = Pusing("###,###.##", WVector1.TextMatrix(Renglon, 7))
                        WVector1.TextMatrix(Renglon, 8) = Str$(rstCtaCtePrv!ivainteres)
                        WVector1.TextMatrix(Renglon, 8) = Pusing("###,###.##", WVector1.TextMatrix(Renglon, 8))
                        ZZReferencia = IIf(IsNull(rstCtaCtePrv!Referencia), "", rstCtaCtePrv!Referencia)
                        WVector1.TextMatrix(Renglon, 9) = ZZReferencia
                        WVector1.TextMatrix(Renglon, 10) = rstCtaCtePrv!Clave
                        WVector1.TextMatrix(Renglon, 11) = rstCtaCtePrv!NroInterno
                    
                    End If
                    
                    .MoveNext
                    
                    If .EOF = True Then
                        Exit Do
                    End If
                    
                Loop
            End If
        End With
        rstCtaCtePrv.Close
    End If
    
    WVector1.TopRow = 1
    WVector1.Row = 1
    WVector1.Col = 1
    
    WVector1.SetFocus
    
    Exit Sub
    
WError:

    WChequeo = "N"
    Resume Next

End Sub


